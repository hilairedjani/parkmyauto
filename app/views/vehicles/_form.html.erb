<h3>Add a vehicle </h3>
<%= form_with model: [@user, @user.vehicles.build] do |f| %>

    <div class="field">
        <%= f.label :license_plate %><br />
        <%= f.text_field :license_plate %>
    </div>

    <div class="field">
        <%= f.label :make %><br />
        <%= f.text_field :make %>
    </div>

   

    <div class="actions">
        <%= f.submit "Add vehicle" %>
    </div>

<% end %>